<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="esprit.tn.pidevrh.session.SessionListController">
    <Label text="Sessions for the selected Formation" />

    <TableView fx:id="sessionTableView" prefWidth="248.0">
        <columns>
            <TableColumn fx:id="dateColumn" text="Date" />
            <TableColumn fx:id="timeColumn" text="Time" />
            <TableColumn fx:id="salleColumn" text="Salle" />
        </columns>


    </TableView>
    <Button onAction="#openAddSession" text="Add Session" />
</VBox>
